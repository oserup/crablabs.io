{
  "headers": [
    {
      "source": "/.well-known/matrix/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/:path((?!.well-known).*)",
      "destination": "https://www.crablabs.io/:path*",
      "permanent": false
    }
  ]
}