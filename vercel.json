{
  "redirects": [
    {
      "source": "/_matrix/:path*",
      "destination": "https://matrix.crablabs.io/_matrix/:path*",
      "permanent": true
    },
    {
      "source": "/_synapse/:path*",
      "destination": "https://matrix.crablabs.io/_synapse/:path*",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/.well-known/matrix/client",
      "headers": [{ "key": "Content-Type", "value": "application/json" }]
    }
  ]
}
