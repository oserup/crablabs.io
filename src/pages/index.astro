---
// components
import Hero from "../components/Hero.astro";
import Layout from "../layouts/Layout.astro";
import Container from "../components/Container.astro";
import { Image } from "astro:assets";

// icons
import electrical from "../assets/icons/electrical.svg"
import software from "../assets/icons/code.svg";
import manufacturing from "../assets/icons/manufacturing.svg";
import engineering from "../assets/icons/engineering.svg";

// Structured data for SEO
const structuredData = {
  "@context": "https://schema.org",
  "@type": ["ProfessionalService", "LocalBusiness"],
  "name": "Electronic Product Development Services - Long Beach, CA",
  "description": "Electronic product development services including PCB design, embedded firmware, and system integration for medical devices, industrial equipment, and scientific instruments.",
  "url": "https://crablabs.io",
  "telephone": "+1-424-271-5569",
  "email": "contact@crablabs.io",
  "address": {
    "@type": "PostalAddress",
    "streetAddress": "235 E Broadway Ste 210",
    "addressLocality": "Long Beach",
    "addressRegion": "CA",
    "postalCode": "90803",
    "addressCountry": "US"
  }
};
---

<!-- ========== MAIN CONTENT ========== -->
<main id="content">
  <Layout title="Electronic Product Development | PCB Design & Embedded Systems | Long Beach, CA">
    <script type="application/ld+json" is:inline set:html={JSON.stringify(structuredData)}></script>
    <Hero />
    <Container>

      <!-- What We Do -->
      <div class="bg-white dark:bg-neutral-900 rounded-2xl p-6 mb-12 border border-slate-200 dark:border-slate-700">
        <h2 class="text-2xl font-bold text-slate-800 dark:text-slate-100 text-center mb-6">
          What We Do
        </h2>
        
        <div class="grid md:grid-cols-2 lg:grid-cols-4 gap-6 mb-6">
          <div class="text-center">
            <div class="w-12 h-12 bg-orange-100 dark:bg-orange-900/30 rounded-xl flex items-center justify-center mb-4 mx-auto">
              <Image src={electrical} alt="Electronics" format={"svg"} class="w-7 h-7" />
            </div>
            <h3 class="text-lg font-bold text-slate-800 dark:text-slate-100 mb-3">Electronics Design</h3>
            <ul class="text-sm text-slate-600 dark:text-slate-400 text-left space-y-1">
              <li>• PCB Design & Layout</li>
              <li>• Analog & Digital Circuits</li>
              <li>• Power Systems</li>
              <li>• RF & Wireless Design</li>
              <li>• Signal Integrity</li>
            </ul>
          </div>

          <div class="text-center">
            <div class="w-12 h-12 bg-blue-100 dark:bg-blue-900/30 rounded-xl flex items-center justify-center mb-4 mx-auto">
              <Image src={software} alt="Software" format={"svg"} class="w-7 h-7" />
            </div>
            <h3 class="text-lg font-bold text-slate-800 dark:text-slate-100 mb-3">Software Development</h3>
            <ul class="text-sm text-slate-600 dark:text-slate-400 text-left space-y-1">
              <li>• Embedded Firmware</li>
              <li>• Real-Time Systems</li>
              <li>• Device Drivers</li>
              <li>• Communication Protocols</li>
              <li>• User Interfaces</li>
            </ul>
          </div>

          <div class="text-center">
            <div class="w-12 h-12 bg-purple-100 dark:bg-purple-900/30 rounded-xl flex items-center justify-center mb-4 mx-auto">
              <Image src={manufacturing} alt="Integration" format={"svg"} class="w-7 h-7" />
            </div>
            <h3 class="text-lg font-bold text-slate-800 dark:text-slate-100 mb-3">System Integration</h3>
            <ul class="text-sm text-slate-600 dark:text-slate-400 text-left space-y-1">
              <li>• Hardware/Software Co-design</li>
              <li>• System Architecture</li>
              <li>• Interface Design</li>
              <li>• Protocol Implementation</li>
              <li>• Performance Optimization</li>
            </ul>
          </div>

          <div class="text-center">
            <div class="w-12 h-12 bg-emerald-100 dark:bg-emerald-900/30 rounded-xl flex items-center justify-center mb-4 mx-auto">
              <Image src={engineering} alt="Testing & Support" format={"svg"} class="w-7 h-7" />
            </div>
            <h3 class="text-lg font-bold text-slate-800 dark:text-slate-100 mb-3">Testing & Support</h3>
            <ul class="text-sm text-slate-600 dark:text-slate-400 text-left space-y-1">
              <li>• Design Verification</li>
              <li>• Pre-Compliance Design Review</li>
              <li>• Technical Documentation</li>
              <li>• Manufacturing Support</li>
              <li>• Post-Launch Assistance</li>
            </ul>
          </div>
        </div>
        
        <div class="text-center">
          <div class="flex flex-col sm:flex-row gap-4 justify-center items-center">
            <a href="/services" class="inline-flex items-center px-6 py-3 bg-slate-100 dark:bg-slate-800 text-slate-700 dark:text-slate-300 font-medium rounded-lg hover:bg-slate-200 dark:hover:bg-slate-700 transition-colors duration-200">
              View our services
              <svg class="w-4 h-4 ml-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7"/>
              </svg>
            </a>
            <a href="https://blog.crablabs.io" target="_blank" class="inline-flex items-center px-6 py-3 bg-slate-600 dark:bg-slate-700 text-white font-medium rounded-lg hover:bg-slate-700 dark:hover:bg-slate-600 transition-colors duration-200">
              <svg class="w-4 h-4 mr-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M15.232 5.232l3.536 3.536m-2.036-5.036a2.5 2.5 0 113.536 3.536L6.5 21.036H3v-3.572L16.732 3.732z"/>
              </svg>
              Technical blog
            </a>
            <a href="https://github.com/crablabsllc" target="_blank" class="inline-flex items-center px-6 py-3 bg-slate-800 dark:bg-slate-700 text-white font-medium rounded-lg hover:bg-slate-900 dark:hover:bg-slate-600 transition-colors duration-200">
              <svg class="w-4 h-4 mr-2" fill="currentColor" viewBox="0 0 24 24">
                <path d="M12 0c-6.626 0-12 5.373-12 12 0 5.302 3.438 9.8 8.207 11.387.599.111.793-.261.793-.577v-2.234c-3.338.726-4.033-1.416-4.033-1.416-.546-1.387-1.333-1.756-1.333-1.756-1.089-.745.083-.729.083-.729 1.205.084 1.839 1.237 1.839 1.237 1.07 1.834 2.807 1.304 3.492.997.107-.775.418-1.305.762-1.604-2.665-.305-5.467-1.334-5.467-5.931 0-1.311.469-2.381 1.236-3.221-.124-.303-.535-1.524.117-3.176 0 0 1.008-.322 3.301 1.23.957-.266 1.983-.399 3.003-.404 1.02.005 2.047.138 3.006.404 2.291-1.552 3.297-1.23 3.297-1.23.653 1.653.242 2.874.118 3.176.77.84 1.235 1.911 1.235 3.221 0 4.609-2.807 5.624-5.479 5.921.43.372.823 1.102.823 2.222v3.293c0 .319.192.694.801.576 4.765-1.589 8.199-6.086 8.199-11.386 0-6.627-5.373-12-12-12z"/>
              </svg>
              View our code
            </a>
          </div>
        </div>
      </div>



      <!-- Why Choose Us -->
      <div class="bg-white dark:bg-neutral-900 rounded-2xl p-6 mb-12 border border-slate-200 dark:border-slate-700">
        <h2 class="text-2xl font-bold text-slate-800 dark:text-slate-100 text-center mb-6">
          Who We Are
        </h2>
        <p class="text-lg text-slate-600 dark:text-slate-300 text-center mb-10 max-w-3xl mx-auto">
          We're different from other engineering options. Here's how we compare:
        </p>
        
        <div class="grid md:grid-cols-2 gap-6 mb-8">
          <!-- Large Consultancies -->
          <div class="bg-slate-50 dark:bg-neutral-800 p-6 rounded-xl border border-slate-200 dark:border-slate-700">
            <h3 class="font-bold text-slate-800 dark:text-slate-100 mb-3">Large Consultancies</h3>
            <div class="space-y-2 text-sm">
              <div class="flex items-start">
                <span class="text-green-500 mr-2">✓</span>
                <span class="text-slate-600 dark:text-slate-400">Large teams for complex projects</span>
              </div>
              <div class="flex items-start">
                <span class="text-green-500 mr-2">✓</span>
                <span class="text-slate-600 dark:text-slate-400">Proven track record with major clients</span>
              </div>
              <div class="flex items-start">
                <span class="text-green-500 mr-2">✓</span>
                <span class="text-slate-600 dark:text-slate-400">Deep bench of specialists</span>
              </div>
              <div class="flex items-start">
                <span class="text-red-500 mr-2">✗</span>
                <span class="text-slate-600 dark:text-slate-400">$200-300/hr (often junior staff)</span>
              </div>
              <div class="flex items-start">
                <span class="text-red-500 mr-2">✗</span>
                <span class="text-slate-600 dark:text-slate-400">Communication through PMs only</span>
              </div>
              <div class="flex items-start">
                <span class="text-red-500 mr-2">✗</span>
                <span class="text-slate-600 dark:text-slate-400">Slower, more bureaucratic</span>
              </div>
            </div>
          </div>
          
          <!-- Our Approach -->
          <div class="bg-gradient-to-br from-orange-50 to-orange-100 dark:from-orange-900/20 dark:to-orange-800/10 p-6 rounded-xl border-2 border-orange-200 dark:border-orange-800/50">
            <h3 class="font-bold text-slate-800 dark:text-slate-100 mb-3">Us</h3>
            <div class="space-y-2 text-sm">
              <div class="flex items-start">
                <span class="text-green-500 mr-2">✓</span>
                <span class="text-slate-600 dark:text-slate-400"><strong>Direct access to senior engineers</strong></span>
              </div>
              <div class="flex items-start">
                <span class="text-green-500 mr-2">✓</span>
                <span class="text-slate-600 dark:text-slate-400"><strong>Fast iteration and decisions</strong></span>
              </div>
              <div class="flex items-start">
                <span class="text-green-500 mr-2">✓</span>
                <span class="text-slate-600 dark:text-slate-400"><strong>40-60% below big firm rates</strong></span>
              </div>
              <div class="flex items-start">
                <span class="text-green-500 mr-2">✓</span>
                <span class="text-slate-600 dark:text-slate-400"><strong>ISO-compliant quality processes</strong></span>
              </div>
              <div class="flex items-start">
                <span class="text-amber-500 mr-2">~</span>
                <span class="text-slate-600 dark:text-slate-400"><strong>Building portfolio with early clients</strong></span>
              </div>
              <div class="flex items-start">
                <span class="text-amber-500 mr-2">~</span>
                <span class="text-slate-600 dark:text-slate-400"><strong>Best for focused projects ($5K-50K)</strong></span>
              </div>
            </div>
          </div>
          
          <!-- Offshore Teams -->
          <div class="bg-slate-50 dark:bg-neutral-800 p-6 rounded-xl border border-slate-200 dark:border-slate-700">
            <h3 class="font-bold text-slate-800 dark:text-slate-100 mb-3">Offshore Teams</h3>
            <div class="space-y-2 text-sm">
              <div class="flex items-start">
                <span class="text-green-500 mr-2">✓</span>
                <span class="text-slate-600 dark:text-slate-400">Very low cost ($20-60/hr)</span>
              </div>
              <div class="flex items-start">
                <span class="text-green-500 mr-2">✓</span>
                <span class="text-slate-600 dark:text-slate-400">Large talent pool available</span>
              </div>
              <div class="flex items-start">
                <span class="text-amber-500 mr-2">~</span>
                <span class="text-slate-600 dark:text-slate-400">Quality varies significantly</span>
              </div>
              <div class="flex items-start">
                <span class="text-amber-500 mr-2">~</span>
                <span class="text-slate-600 dark:text-slate-400">Project management (rarely)</span>
              </div>
              <div class="flex items-start">
                <span class="text-red-500 mr-2">✗</span>
                <span class="text-slate-600 dark:text-slate-400">Time zone challenges</span>
              </div>
            </div>
          </div>
          
