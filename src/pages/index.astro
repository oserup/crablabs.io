---
// components
import Hero from "../components/Hero.astro";
import Layout from "../layouts/Layout.astro";
import Container from "../components/Container.astro";


// Main business structured data for SEO
const businessData = {
  "@context": "https://schema.org",
  "@type": ["ProfessionalService", "LocalBusiness", "TechnologyConsultingService"],
  "name": "Crab Labs - Electronic Device Engineering",
  "alternateName": "Crab Labs LLC",
  "description": "5 senior electronics engineers providing direct-access embedded systems development. Specializing in regulatory-compliant medical devices, industrial automation, and scientific instruments with 30+ years combined experience.",
  "url": "https://crablabs.io",
  "telephone": "+1-424-271-5569",
  "email": "contact@crablabs.io",
  "foundingDate": "2024",
  "numberOfEmployees": "5",
  "priceRange": "$$$",
  "paymentAccepted": ["Cash", "Credit Card", "Wire Transfer"],
  "currenciesAccepted": "USD",
  "serviceArea": {
    "@type": "Country",
    "name": "United States"
  },
  "areaServed": "US",
  "address": {
    "@type": "PostalAddress",
    "streetAddress": "235 E Broadway Ste 210",
    "addressLocality": "Long Beach",
    "addressRegion": "CA",
    "postalCode": "90803",
    "addressCountry": "US"
  },
  "geo": {
    "@type": "GeoCoordinates",
    "latitude": "33.7701",
    "longitude": "-118.1937"
  },
  "openingHours": "Mo-Fr 09:00-17:00",
  "knowsAbout": [
    "PCB Design",
    "Embedded Systems",
    "Medical Device Engineering",
    "FDA Compliance",
    "IEC 60601",
    "Industrial Automation",
    "EMC Testing",
    "Regulatory Compliance"
  ],
  "hasOfferCatalog": {
    "@type": "OfferCatalog",
    "name": "Engineering Services",
    "itemListElement": [
      {
        "@type": "Offer",
        "itemOffered": {
          "@type": "Service",
          "name": "Design Review",
          "description": "Independent analysis of electronic designs before production"
        },
        "priceSpecification": {
          "@type": "PriceSpecification",
          "price": "1000",
          "priceCurrency": "USD",
          "description": "Starting at $1k"
        }
      },
      {
        "@type": "Offer",
        "itemOffered": {
          "@type": "Service",
          "name": "Product Development",
          "description": "Complete electrical and firmware development from concept to production"
        },
        "priceSpecification": {
          "@type": "PriceSpecification",
          "price": "10000",
          "priceCurrency": "USD",
          "description": "Starting at $10k"
        }
      },
      {
        "@type": "Offer",
        "itemOffered": {
          "@type": "Service",
          "name": "Testing & Compliance",
          "description": "EMC, EMI, and environmental pre-certification testing"
        },
        "priceSpecification": {
          "@type": "PriceSpecification",
          "price": "2500",
          "priceCurrency": "USD",
          "description": "Starting at $2.5k"
        }
      }
    ]
  }
};

// FAQ Schema for common engineering questions
const faqData = {
  "@context": "https://schema.org",
  "@type": "FAQPage",
  "mainEntity": [
    {
      "@type": "Question",
      "name": "What is the typical cost for PCB design services?",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "Our PCB design services start at $1,000 for design reviews and $10,000 for complete product development. Pricing depends on complexity, layer count, and regulatory requirements. We provide fixed-price quotes after initial consultation."
      }
    },
    {
      "@type": "Question",
      "name": "Do you work with medical device companies?",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "Yes, 42% of our projects are medical devices. We specialize in IEC 60601 compliance, FDA submission support, and have completed 12+ FDA 510(k) submissions. Our team has 30+ years experience in regulatory-compliant embedded systems."
      }
    },
    {
      "@type": "Question",
      "name": "What makes Crab Labs different from other electronics engineering firms?",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "We provide direct access to 5 senior engineers with no middlemen or project managers. Fixed pricing, 30+ years combined experience, and we own a professional EMC testing lab for faster iteration. Projects range from $1k-$100k."
      }
    },
    {
      "@type": "Question",
      "name": "How long does embedded firmware development take?",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "Firmware development typically takes 1-18 months depending on complexity. Simple device drivers: 1-4 weeks. Complex safety-critical systems: 6-18 months. We follow IEC 62304 for medical software and MISRA C:2012 for safety-critical applications."
      }
    },
    {
      "@type": "Question",
      "name": "Do you provide EMC testing and compliance services?",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "Yes, we have a professional 1,000 sq ft EMC testing lab with 8 GHz oscilloscopes, spectrum analyzers, and EMC tent. We provide pre-compliance testing starting at $2,500 to identify issues before expensive certification."
      }
    }
  ]
};

// Organization schema for better business visibility
const organizationData = {
  "@context": "https://schema.org",
  "@type": "Organization",
  "name": "Crab Labs LLC",
  "legalName": "Crab Labs LLC",
  "url": "https://crablabs.io",
  "logo": "https://crablabs.io/logo.svg",
  "image": "https://crablabs.io/android-chrome-512x512.png",
  "description": "Professional electronics engineering firm specializing in regulatory-compliant embedded systems for medical devices, industrial automation, and scientific instruments.",
  "telephone": "+1-424-271-5569",
  "email": "contact@crablabs.io",
  "address": {
    "@type": "PostalAddress",
    "streetAddress": "235 E Broadway Ste 210",
    "addressLocality": "Long Beach",
    "addressRegion": "CA",
    "postalCode": "90803",
    "addressCountry": "US"
  },
  "foundingDate": "2024",
  "numberOfEmployees": "5",
  "slogan": "When precision matters, experience delivers",
  "knowsAbout": [
    "Electronics Engineering",
    "PCB Design",
    "Embedded Systems",
    "Medical Device Engineering",
    "FDA 510k Submissions",
    "IEC 60601 Compliance",
    "Industrial Automation",
    "EMC Testing",
    "Firmware Development",
    "Regulatory Compliance",
    "Scientific Instruments"
  ],
  "memberOf": [
    {
      "@type": "Organization",
      "name": "IEEE",
      "description": "Institute of Electrical and Electronics Engineers"
    }
  ],
  "sameAs": [
    "https://linkedin.com/company/crablabs",
    "https://github.com/crablabs"
  ]
};
---

<main id="content">
  <Layout
    title="Electronic Device Engineering | Crab Labs"
    description="5 senior electronics engineers, 30+ years experience. Medical device, industrial & scientific embedded systems. Fixed prices $1k-$100k. Direct engineer access, no middlemen. Long Beach, CA."
  >
    <script type="application/ld+json" is:inline set:html={JSON.stringify(businessData)}></script>
    <script type="application/ld+json" is:inline set:html={JSON.stringify(faqData)}></script>
    <script type="application/ld+json" is:inline set:html={JSON.stringify(organizationData)}></script>

    <!-- Page uses global styles -->

    <!-- Content with proper spacing for fixed navbar -->
    <div class="page-container">
      <Hero />
    </div>

    <!-- Clean Minimal Background -->
    <div class="absolute inset-0 -z-10 bg-white"></div>

    <div class="py-8"></div>

    <Container>
      <!-- Separator -->
      <div class="content-container mb-24">
        <div class="h-px bg-gray-200"></div>
      </div>

      <!-- What We Do -->
      <section class="relative mb-section">
        <div class="section-white">
          <div class="content-container">
            <h2 class="heading-section">What We Do</h2>

            <div class="text-center max-w-5xl mx-auto">
              <div class="grid md:grid-cols-3 gap-16">
                <div class="flex flex-col items-center text-center p-4 rounded-lg hover:bg-gray-50 transition-colors duration-200">
                  <div class="w-20 h-20 bg-orange-100 rounded-lg flex items-center justify-center mb-8 hover:bg-orange-200 hover:scale-105 transition-all duration-200">
                    <svg class="w-8 h-8 text-orange-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                      <path stroke-linecap="round" stroke-linejoin="round" stroke-width="1.5" d="M9 3v2m6-2v2M9 19v2m6-2v2M5 9H3m2 6H3m18-6h-2m2 6h-2M7 19h10a2 2 0 002-2V7a2 2 0 00-2-2H7a2 2 0 00-2 2v10a2 2 0 002 2zM9 9h6v6H9V9z"/>
                    </svg>
                  </div>
                  <div>
                    <h3 class="heading-card">Hardware Design</h3>
                    <p class="text-body">
                      Circuit boards and component selection
                    </p>
                  </div>
                </div>

                <div class="flex flex-col items-center text-center p-4 rounded-lg hover:bg-gray-50 transition-colors duration-200">
                  <div class="w-20 h-20 bg-orange-100 rounded-lg flex items-center justify-center mb-8 hover:bg-orange-200 hover:scale-105 transition-all duration-200">
                    <svg class="w-8 h-8 text-orange-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                      <path stroke-linecap="round" stroke-linejoin="round" stroke-width="1.5" d="M10 20l4-16m4 4l4 4-4 4M6 16l-4-4 4-4"/>
                    </svg>
                  </div>
                  <div>
                    <h3 class="heading-card">Firmware Development</h3>
                    <p class="text-body">
                      Code that controls your device
                    </p>
                  </div>
                </div>

                <div class="flex flex-col items-center text-center p-4 rounded-lg hover:bg-gray-50 transition-colors duration-200">
                  <div class="w-20 h-20 bg-orange-100 rounded-lg flex items-center justify-center mb-8 hover:bg-orange-200 hover:scale-105 transition-all duration-200">
                    <svg class="w-8 h-8 text-orange-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                      <path stroke-linecap="round" stroke-linejoin="round" stroke-width="1.5" d="M9 12l2 2 4-4M7.835 4.697a3.42 3.42 0 001.946-.806 3.42 3.42 0 014.438 0 3.42 3.42 0 001.946.806 3.42 3.42 0 013.138 3.138 3.42 3.42 0 00.806 1.946 3.42 3.42 0 010 4.438 3.42 3.42 0 00-.806 1.946 3.42 3.42 0 01-3.138 3.138 3.42 3.42 0 00-1.946.806 3.42 3.42 0 01-4.438 0 3.42 3.42 0 00-1.946-.806 3.42 3.42 0 01-3.138-3.138 3.42 3.42 0 00-.806-1.946 3.42 3.42 0 010-4.438 3.42 3.42 0 00.806-1.946 3.42 3.42 0 013.138-3.138z"/>
                    </svg>
                  </div>
                  <div>
                    <h3 class="heading-card">Integration & Testing</h3>
                    <p class="text-body">
                      Making everything work together
                    </p>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </section>

<!-- Industries We Serve -->
      <section class="relative mb-section">
        <div class="section-gray">
          <div class="content-container">
            <h2 class="heading-section">Who We Help</h2>

            <!-- Clean Progress Bar -->
            <div class="mb-20 px-4">
              <div class="flex h-20 rounded-xl overflow-hidden bg-white shadow-sm border border-gray-200">
                <!-- Medical: 42% -->
                <div class="bg-orange-500 flex items-center justify-center text-white font-semibold text-base" style="width: 42%">
                  42% Medical
                </div>
                <!-- Industrial/Robotics: 35% -->
                <div class="bg-gray-600 flex items-center justify-center text-white font-semibold text-base" style="width: 35%">
                  35% Industrial
                </div>
                <!-- Scientific: 20% -->
                <div class="bg-gray-700 flex items-center justify-center text-white font-semibold text-base" style="width: 20%">
                  20% Scientific
                </div>
                <!-- Consumer Electronics: 3% -->
                <div class="bg-gray-400 flex items-center justify-center text-white font-semibold text-sm" style="width: 3%">
                  3%
                </div>
              </div>
            </div>

            <div class="grid md:grid-cols-4 gap-8">
              <div class="text-center p-8 bg-white rounded-lg border border-gray-200 hover:shadow-md hover:border-orange-200 transition-all duration-200">
                <div class="w-16 h-16 bg-orange-500 rounded-lg flex items-center justify-center mx-auto mb-6">
                  <svg class="w-7 h-7 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="1.5" d="M4.318 6.318a4.5 4.5 0 000 6.364L12 20.364l7.682-7.682a4.5 4.5 0 00-6.364-6.364L12 7.636l-1.318-1.318a4.5 4.5 0 00-6.364 0z"/>
                  </svg>
                </div>
                <h3 class="heading-subsection">Medical Devices</h3>
                <p class="text-gray-600">Diagnostic and therapeutic devices</p>
              </div>

              <div class="text-center p-8 bg-white rounded-lg border border-gray-200 hover:shadow-md hover:border-orange-200 transition-all duration-200">
                <div class="w-20 h-20 bg-gray-600 rounded-lg flex items-center justify-center mx-auto mb-6">
                  <svg class="w-8 h-8 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="1.5" d="M10.325 4.317c.426-1.756 2.924-1.756 3.35 0a1.724 1.724 0 002.573 1.066c1.543-.94 3.31.826 2.37 2.37a1.724 1.724 0 001.065 2.572c1.756.426 1.756 2.924 0 3.35a1.724 1.724 0 00-1.066 2.573c.94 1.543-.826 3.31-2.37 2.37a1.724 1.724 0 00-2.572 1.065c-.426 1.756-2.924 1.756-3.35 0a1.724 1.724 0 00-2.573-1.066c-1.543.94-3.31-.826-2.37-2.37a1.724 1.724 0 00-1.065-2.572c-1.756-.426-1.756-2.924 0-3.35a1.724 1.724 0 001.066-2.573c-.94-1.543.826-3.31 2.37-2.37.996.608 2.296.07 2.572-1.065z"/>
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="1.5" d="M15 12a3 3 0 11-6 0 3 3 0 016 0z"/>
                  </svg>
                </div>
                <h3 class="heading-subsection">Industrial</h3>
                <p class="text-gray-600">Manufacturing automation, control systems</p>
              </div>

              <div class="text-center p-8 bg-white rounded-lg border border-gray-200 hover:shadow-md hover:border-orange-200 transition-all duration-200">
                <div class="w-20 h-20 bg-gray-700 rounded-lg flex items-center justify-center mx-auto mb-6">
                  <svg class="w-8 h-8 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="1.5" d="M9.663 17h4.673M12 3v1m6.364 1.636l-.707.707M21 12h-1M4 12H3m3.343-5.657l-.707-.707m2.828 9.9a5 5 0 117.072 0l-.548.547A3.374 3.374 0 0014 18.469V19a2 2 0 11-4 0v-.531c0-.895-.356-1.754-.988-2.386l-.548-.547z"/>
                  </svg>
                </div>
                <h3 class="heading-subsection">Scientific Instruments</h3>
                <p class="text-gray-600">Research equipment, test instruments</p>
              </div>

              <div class="text-center p-8 bg-white rounded-lg border border-gray-200 hover:shadow-md hover:border-orange-200 transition-all duration-200">
                <div class="w-20 h-20 bg-gray-400 rounded-lg flex items-center justify-center mx-auto mb-6">
                  <svg class="w-8 h-8 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="1.5" d="M12 18h.01M8 21h8a2 2 0 002-2V5a2 2 0 00-2-2H8a2 2 0 00-2 2v14a2 2 0 002 2z"/>
                  </svg>
                </div>
                <h3 class="heading-subsection">Consumer Electronics</h3>
                <p class="text-gray-600">Smart devices, IoT products</p>
              </div>
            </div>
          </div>
        </div>
      </section>

      <!-- Separator -->
      <div class="content-container">
        <div class="h-px bg-gray-200"></div>
      </div>

      <!-- Why Choose Us -->
      <section class="relative mb-section">
        <div class="section-white">
          <div class="content-container">
            <h2 class="heading-section">Why Choose Us</h2>

            <div class="grid md:grid-cols-2 lg:grid-cols-4 gap-8">
              <div class="text-center p-4 rounded-lg hover:bg-gray-50 transition-colors duration-200">
                <div class="w-20 h-20 bg-gray-100 rounded-lg flex items-center justify-center mb-8 mx-auto hover:bg-orange-100 transition-colors duration-200">
                  <svg class="w-8 h-8 text-gray-700" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="1.5" d="M9 12l2 2 4-4m6 2a9 9 0 11-18 0 9 9 0 0118 0z"/>
                  </svg>
                </div>
                <h3 class="heading-card">We Know the Rules</h3>
                <p class="text-body">Built-in regulatory compliance from the start</p>
              </div>

              <div class="text-center p-4 rounded-lg hover:bg-gray-50 transition-colors duration-200">
                <div class="w-20 h-20 bg-gray-100 rounded-lg flex items-center justify-center mb-8 mx-auto hover:bg-orange-100 transition-colors duration-200">
                  <svg class="w-8 h-8 text-gray-700" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="1.5" d="M19 21V5a2 2 0 00-2-2H7a2 2 0 00-2 2v16m14 0h2m-2 0h-5m-9 0H3m2 0h5M9 7h1m-1 4h1m4-4h1m-1 4h1m-5 10v-5a1 1 0 011-1h2a1 1 0 011 1v5m-4 0h4"/>
                  </svg>
                </div>
                <h3 class="heading-card">In-House Testing</h3>
                <p class="text-body">Find problems early, save money later</p>
              </div>

              <div class="text-center p-4 rounded-lg hover:bg-gray-50 transition-colors duration-200">
                <div class="w-20 h-20 bg-gray-100 rounded-lg flex items-center justify-center mb-8 mx-auto hover:bg-orange-100 transition-colors duration-200">
                  <svg class="w-8 h-8 text-gray-700" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="1.5" d="M17 20h5v-2a3 3 0 00-5.356-1.857M17 20H7m10 0v-2c0-.656-.126-1.283-.356-1.857M7 20H2v-2a3 3 0 015.356-1.857M7 20v-2c0-.656.126-1.283.356-1.857m0 0a5.002 5.002 0 019.288 0M15 7a3 3 0 11-6 0 3 3 0 016 0zm6 3a2 2 0 11-4 0 2 2 0 014 0zM7 10a2 2 0 11-4 0 2 2 0 014 0z"/>
                  </svg>
                </div>
                <h3 class="heading-card">No Sales Team</h3>
                <p class="text-body">Work directly with the engineers</p>
              </div>

              <div class="text-center p-4 rounded-lg hover:bg-gray-50 transition-colors duration-200">
                <div class="w-20 h-20 bg-gray-100 rounded-lg flex items-center justify-center mb-8 mx-auto hover:bg-orange-100 transition-colors duration-200">
                  <svg class="w-8 h-8 text-gray-700" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="1.5" d="M15 7a2 2 0 012 2m4 0a6 6 0 01-7.743 5.743L11 17H9v2H7v2H4a1 1 0 01-1-1v-2.586a1 1 0 01.293-.707l5.964-5.964A6 6 0 1121 9z"/>
                  </svg>
                </div>
                <h3 class="heading-card">You Own Everything</h3>
                <p class="text-body">Complete IP transfer upon delivery</p>
              </div>
            </div>
          </div>
        </div>
      </section>

      <!-- Separator -->
      <div class="content-container">
        <div class="h-px bg-gray-200"></div>
      </div>

      <!-- Learn More -->
      <section class="relative mb-section">
        <div class="section-white">
          <div class="content-container" style="text-align: center;">
            <h2 class="heading-section">Explore More</h2>

            <div class="grid md:grid-cols-4 gap-6">
              <a href="/services" class="bg-white rounded-2xl p-8 border border-gray-100 hover:shadow-md hover:border-orange-200 transition-all duration-200">
                <div class="w-20 h-20 bg-orange-100 rounded-lg flex items-center justify-center mx-auto mb-8 hover:bg-orange-200 hover:scale-105 transition-all duration-200">
                  <svg class="w-8 h-8 text-orange-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="1.5" d="M9 5H7a2 2 0 00-2 2v10a2 2 0 002 2h8a2 2 0 002-2V7a2 2 0 00-2-2h-2M9 5a2 2 0 002 2h2a2 2 0 002-2M9 5a2 2 0 012-2h2a2 2 0 012 2m-5 9l2 2 4-4"/>
                  </svg>
                </div>
                <h3 class="heading-card">Services</h3>
                <p class="text-body">What we do and how much it costs</p>
              </a>

              <a href="/capabilities" class="bg-white rounded-2xl p-8 border border-gray-100 hover:shadow-md hover:border-orange-200 transition-all duration-200">
                <div class="w-20 h-20 bg-orange-100 rounded-lg flex items-center justify-center mx-auto mb-8 hover:bg-orange-200 hover:scale-105 transition-all duration-200">
                  <svg class="w-8 h-8 text-orange-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="1.5" d="M19.428 15.428a2 2 0 00-1.022-.547l-2.387-.477a6 6 0 00-3.86.517l-.318.158a6 6 0 01-3.86.517L6.05 15.21a2 2 0 00-1.806.547M8 4h8l-1 1v5.172a2 2 0 00.586 1.414l5 5c1.26 1.26.367 3.414-1.415 3.414H4.828c-1.782 0-2.674-2.154-1.414-3.414l5-5A2 2 0 009 10.172V5L8 4z"/>
                  </svg>
                </div>
                <h3 class="heading-card">Capabilities</h3>
                <p class="text-body">Our tools and testing lab</p>
              </a>

              <a href="/about" class="bg-white rounded-2xl p-8 border border-gray-100 hover:shadow-md hover:border-orange-200 transition-all duration-200">
                <div class="w-20 h-20 bg-orange-100 rounded-lg flex items-center justify-center mx-auto mb-8 hover:bg-orange-200 hover:scale-105 transition-all duration-200">
                  <svg class="w-8 h-8 text-orange-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="1.5" d="M17 20h5v-2a3 3 0 00-5.356-1.857M17 20H7m10 0v-2c0-.656-.126-1.283-.356-1.857M7 20H2v-2a3 3 0 015.356-1.857M7 20v-2c0-.656.126-1.283.356-1.857m0 0a5.002 5.002 0 019.288 0M15 7a3 3 0 11-6 0 3 3 0 016 0zm6 3a2 2 0 11-4 0 2 2 0 014 0zM7 10a2 2 0 11-4 0 2 2 0 014 0z"/>
                  </svg>
                </div>
                <h3 class="heading-card">About</h3>
                <p class="text-body">Our team and past projects</p>
              </a>

              <a href="/case-studies" class="bg-white rounded-2xl p-8 border border-gray-100 hover:shadow-md hover:border-orange-200 transition-all duration-200">
