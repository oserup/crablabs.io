<style>
   #traces {
      padding: 0;
      transform: scale(1.15) translateY(-90px);
      transform-origin: top center;
   }

   @media (max-width: 767px) {
      #traces {
         position: absolute;
         top: -2rem;
         right: -4rem;
         width: 140%;
         height: auto;
         transform: none;
      }
   }

   /* ── Base trace ── */
   .trace {
      stroke-dasharray: 600;
      stroke-dashoffset: 600;
      stroke-opacity: 0;
      animation-fill-mode: forwards;
   }

   /* ── Signal traces: thin, fast ── */
   .trace-signal {
      animation: routeSignal 2s cubic-bezier(0.25, 0.1, 0.25, 1) forwards;
   }

   /* ── Bus traces: medium, steady ── */
   .trace-bus {
      animation: routeBus 2.5s cubic-bezier(0.25, 0.1, 0.25, 1) forwards;
   }

   /* ── Power traces: thick, deliberate ── */
   .trace-power {
      animation: routePower 3s cubic-bezier(0.25, 0.1, 0.25, 1) forwards;
   }

   /* Sequential routing — like an autorouter working through nets */
   .trace:nth-child(1)  { animation-delay: 0.1s; }
   .trace:nth-child(2)  { animation-delay: 0.3s; }
   .trace:nth-child(3)  { animation-delay: 0.5s; }
   .trace:nth-child(4)  { animation-delay: 0.7s; }
   .trace:nth-child(5)  { animation-delay: 0.9s; }
   .trace:nth-child(6)  { animation-delay: 1.1s; }
   .trace:nth-child(7)  { animation-delay: 1.3s; }
   .trace:nth-child(8)  { animation-delay: 1.5s; }
   .trace:nth-child(9)  { animation-delay: 1.7s; }
   .trace:nth-child(10) { animation-delay: 1.9s; }
   .trace:nth-child(11) { animation-delay: 2.1s; }
   .trace:nth-child(12) { animation-delay: 2.3s; }
   .trace:nth-child(13) { animation-delay: 2.5s; }
   .trace:nth-child(14) { animation-delay: 2.7s; }
   .trace:nth-child(15) { animation-delay: 2.9s; }

   /* ── Signal: crisp fade-in draw ── */
   @keyframes routeSignal {
      0% {
         stroke-dashoffset: 600;
         stroke-opacity: 0;
      }
      8% {
         stroke-opacity: 0.5;
      }
      100% {
         stroke-dashoffset: 0;
         stroke-opacity: 0.55;
      }
   }

   /* ── Bus: steady draw ── */
   @keyframes routeBus {
      0% {
         stroke-dashoffset: 600;
         stroke-opacity: 0;
      }
      8% {
         stroke-opacity: 0.4;
      }
      100% {
         stroke-dashoffset: 0;
         stroke-opacity: 0.7;
      }
   }

   /* ── Power: bold draw ── */
   @keyframes routePower {
      0% {
         stroke-dashoffset: 600;
         stroke-opacity: 0;
      }
      5% {
         stroke-opacity: 0.4;
      }
      70% {
         stroke-opacity: 0.95;
      }
      100% {
         stroke-dashoffset: 0;
         stroke-opacity: 0.85;
      }
   }
</style>
