<!-- Apple-inspired smooth trace animation -->
<style>
   #traces {
      border-radius: 24px;
      padding: 20px;
      margin-top: -12rem;
      padding-top: 4rem;
   }

   path {
      stroke-dasharray: 500;
      stroke-dashoffset: 500;
      animation: flowTrace 3s cubic-bezier(0.4, 0, 0.2, 1) forwards;
      transition: all 0.4s ease;
   }

   /* PCB traces animate in sequence */
   path:nth-child(2) { animation-delay: 0s; animation-duration: 2.5s; }
   path:nth-child(3) { animation-delay: 0.1s; animation-duration: 2.6s; }
   path:nth-child(4) { animation-delay: 0.2s; animation-duration: 2.7s; }
   path:nth-child(5) { animation-delay: 0.3s; animation-duration: 2.8s; }
   path:nth-child(6) { animation-delay: 0.4s; animation-duration: 2.9s; }
   path:nth-child(7) { animation-delay: 0.5s; animation-duration: 3.0s; }
   path:nth-child(8) { animation-delay: 0.6s; animation-duration: 3.1s; }
   path:nth-child(9) { animation-delay: 0.7s; animation-duration: 3.2s; }
   path:nth-child(10) { animation-delay: 0.8s; animation-duration: 3.3s; }
   path:nth-child(11) { animation-delay: 0.9s; animation-duration: 3.4s; }
   path:nth-child(12) { animation-delay: 1.0s; animation-duration: 3.5s; }
   path:nth-child(13) { animation-delay: 1.1s; animation-duration: 3.6s; }

   @keyframes flowTrace {
      0% {
         stroke-dashoffset: 500;
         stroke-opacity: 0;
         stroke-width: 2;
      }
      20% {
         stroke-opacity: 0.4;
         stroke-width: 2.5;
      }
      60% {
         stroke-opacity: 0.8;
         stroke-width: 2.8;
      }
      100% {
         stroke-dashoffset: 0;
         stroke-opacity: 0.9;
         stroke-width: 2.5;
      }
   }

   /* Clean hover effect */
   #traces:hover path {
      stroke-opacity: 1;
      stroke-width: 3;
      transition: all 0.3s ease;
   }

   /* Add slight animation on completed paths */
   #traces path {
      animation-fill-mode: forwards;
   }
</style>
